#include <iostream>
#include <string.h>
#include <regex>
 
using namespace std;
 
int main() {
 
 string s = "i went on news. today, it wasn't as good as https://repl.it/languages/python3";
 
 regex r("(https?://)?[^ ]+[.][^ ]+(/[^ ])*");
 
 smatch m;
 
 regex_search(s, m, r);
 
 for (auto x : m)
       cout << x << " ";
    return 0;
 
}
